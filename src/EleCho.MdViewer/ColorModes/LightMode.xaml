<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:md="clr-namespace:EleCho.MdViewer.Markdown">
    <Brush x:Key="{x:Static md:MarkdownResKey.MainForeground}">#1f2328</Brush>
    <Brush x:Key="{x:Static md:MarkdownResKey.HeadingForeground}">#69f</Brush>

    <Brush x:Key="{x:Static md:MarkdownResKey.CodeBlockForeground}">#1f2328</Brush>
    <Brush x:Key="{x:Static md:MarkdownResKey.CodeBlockBackground}">#1C000000</Brush>

    <Brush x:Key="{x:Static md:MarkdownResKey.CodeInlineForeground}">#1f2328</Brush>
    <Brush x:Key="{x:Static md:MarkdownResKey.CodeInlineBackground}">#8C6699FF</Brush>

    <Brush x:Key="{x:Static md:MarkdownResKey.QuoteBlockForeground}">#1f2328</Brush>
    <Brush x:Key="{x:Static md:MarkdownResKey.QuoteBlockBackground}">Transparent</Brush>
    <Brush x:Key="{x:Static md:MarkdownResKey.QuoteBlockBorder}">#8C6699FF</Brush>

    <Brush x:Key="{x:Static md:MarkdownResKey.TableForeground}">#1f2328</Brush>
    <Brush x:Key="{x:Static md:MarkdownResKey.TableBackground}">#e8e7e7</Brush>
    <Brush x:Key="{x:Static md:MarkdownResKey.TableStripe}">#0C000000</Brush>
    <Brush x:Key="{x:Static md:MarkdownResKey.TableBorder}">#dadada</Brush>


    <Brush x:Key="{x:Static md:MarkdownResKey.ThematicBreak}">#c5c5c5</Brush>
    <Brush x:Key="{x:Static md:MarkdownResKey.Mark}">#8C6699FF</Brush>

    <Brush x:Key="{x:Static md:MarkdownResKey.Link}">#9F6699FF</Brush>
    <Brush x:Key="{x:Static md:MarkdownResKey.LinkHover}">#6699FF</Brush>

    <Brush x:Key="{x:Static md:MarkdownResKey.CommonControlMask}">#1C000000</Brush>
    <Brush x:Key="{x:Static md:MarkdownResKey.CommonControlBorder}">#dadada</Brush>
</ResourceDictionary>